{% extends "admin/base.html.twig" %}

{% block main %}

    {% for category in categories %}
        <img src="{{ asset('images/' ~ category.image) }}" alt="{{ category.name }}" title="{{ category.name }}">
        <h2><a href="{{ path ("admin_show_category", {'id' : category.id })}}">{{ category.name }}</a></h2>
    {% endfor %}

{% endblock %}